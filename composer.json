{
    "name": "malefici/test-ci",
    "description": "Test repository for education purpose",
    "type": "project",
    "keywords": ["ci", "education", "symfony"],
    "autoload": {
        "psr-4": {
            "Malefici\\TestCi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Malefici\\TestCi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Malefici",
            "email": "nikita@malefici.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "symfony/config": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/web-profiler-bundle": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/console": "^7.0",
        "symfony/orm-pack": "^2.4",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "7.1.x-dev",
        "symfony/test-pack": "^1.1",
        "symfony/maker-bundle": "^1.52"
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": {
            "test": "vendor/bin/simple-phpunit tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
